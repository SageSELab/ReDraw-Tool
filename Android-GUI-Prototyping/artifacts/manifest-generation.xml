
<?xml version="1.0" encoding="utf-8"?>

<!--Aside from <manifest> and <application>, we don't need any of these-->
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="apps.guiproto.cs435.testapplication">

	<!--Our app shouldn't need any permissions, so we don't care about these-->
    <uses-permission />
    <permission />
    <permission-tree />
    <permission-group />
	<!--Some kind of profiling thing, I don't care -->
    <instrumentation />
	<!--SDK Versions - we use 19 for now-->
    <uses-sdk android:minSdkVersion="10" 
              android:minSdkVersion="19" 
              android:minSdkVersion="25" />

	<!--Hardware/software features needed; don't include these-->
    <uses-configuration />  
    <uses-feature />  
    <supports-screens android:resizeable=["true"| "false"]
                      android:smallScreens=["true" | "false"]
                      android:normalScreens=["true" | "false"]
                      android:largeScreens=["true" | "false"]
                      android:xlargeScreens=["true" | "false"]
                      android:anyDensity=["true" | "false"]
                      android:requiresSmallestWidthDp="integer"
                      android:compatibleWidthLimitDp="integer"
                      android:largestWidthLimitDp="integer"/>
	<!--Might not need to use this-->
    <compatible-screens />  
	<!--OpenGL Texture Compression-->
    <supports-gl-texture />  

    <application
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
